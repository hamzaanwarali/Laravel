APP_NAME=Laravel
APP_ENV=local  # سيتم تغييرها إلى `production` عند الرفع على 
APP_KEY=       # اتركه فارغًا (سيتم توليده تلقائيًا عند النشر)
APP_DEBUG=true # غيّرها إلى `false` في الإنتاج
APP_URL=http://localhost

LOG_CHANNEL=stack
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=pgsql  # غيّرها إلى `pgsql` إذا استخدمت PostgreSQL في 
DB_HOST=127.0.0.1
DB_PORT=5432         # غيّرها إلى `5432` لـ PostgreSQL
DB_DATABASE=laravel
DB_USERNAME=root
DB_PASSWORD=

BROADCAST_DRIVER=log
CACHE_DRIVER=file    # غيّرها إلى `array` في 
SESSION_DRIVER=file   # غيّرها إلى `database` في 
SESSION_LIFETIME=120

QUEUE_CONNECTION=sync

MEMCACHED_HOST=127.0.0.1

REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

